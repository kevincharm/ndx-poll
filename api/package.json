{
  "name": "ndx-poll-api",
  "version": "1.0.0",
  "description": "NDX Poll API.",
  "scripts": {
    "build": "yarn tsc",
    "dev:start": "yarn ts-node-dev src/index.ts",
    "dev:up": "docker-compose -f docker-compose.yml --project-name=ndx-poll-api up -d && docker ps",
    "dev:down": "docker-compose --project-name=ndx-poll-api down && docker ps",
    "dev:migrate": "yarn ts-node ./src/db/migrator.ts",
    "check:format": "yarn prettier --check \"src/**/*.ts\"",
    "check:compile": "yarn tsc --noEmit",
    "test": "LOG_LEVEL=silent yarn env-cmd -f .env.test yarn jest --forceExit --runInBand --detectOpenHandles",
    "test:watch": "LOG_LEVEL=silent yarn env-cmd -f .env.test yarn jest --watch --forceExit --runInBand --detectOpenHandles",
    "test:up": "docker-compose -f docker-compose.test.yml --project-name=ndx-poll-api-test up -d && docker ps",
    "test:down": "docker-compose --project-name=ndx-poll-api-test down && docker ps",
    "test:migrate": "yarn env-cmd -f .env.test ts-node src/db/migrator.ts",
    "ci:test": "LOG_LEVEL=silent yarn env-cmd -f .env.ci yarn jest --ci --colors --coverage --forceExit --runInBand --detectOpenHandles",
    "ci:test:migrate": "yarn env-cmd -f .env.ci ts-node src/db/migrator.ts",
    "prod:migrate": "node src/db/migrator.js",
    "prod:start": "node src/index.js"
  },
  "author": "Kevin Tjiam <kevin@kevincharm.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.7",
    "@types/express-pino-logger": "^4.0.2",
    "@types/jest": "^26.0.5",
    "@types/knex": "^0.16.1",
    "@types/node": "12",
    "@types/object-hash": "^1.3.4",
    "@types/passport": "^1.0.4",
    "@types/passport-http-bearer": "^1.0.35",
    "@types/passport-strategy": "^0.2.35",
    "@types/pino": "^6.3.0",
    "@types/qs": "^6.9.3",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.0.0",
    "@types/ws": "^7.2.7",
    "env-cmd": "^10.1.0",
    "jest": "^26.1.0",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-node-dev": "^1.0.0-pre.51",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "argon2": "^0.26.2",
    "aws-sdk": "^2.716.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.15.0",
    "eth-sig-util": "^3.0.1",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "fast-json-stable-stringify": "^2.1.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "knex": "^0.21.2",
    "object-hash": "^2.1.1",
    "objection": "^2.2.1",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.3.0",
    "pino": "^6.3.2",
    "pino-pretty": "^4.0.2",
    "qs": "^6.9.4",
    "rate-limiter-flexible": "^2.1.9",
    "uuid": "^8.2.0",
    "ws": "^7.3.1",
    "zod": "^1.11.9"
  }
}
